  shorttitle = {Genomic {{Surveillance}} for {{SARS-CoV-2 Variants}}},
  author = {Ma, Kevin C. and Castro, Juan and Lambrou, Anastasia S. and Rose, Erica Billig and Cook, Peter W. and Batra, Dhwani and Cubenas, Caelin and Hughes, Laura J. and MacCannell, Duncan R. and Mandal, Paritra and Mittal, Neha and Sheth, Mili and Smith, Casey and Winn, Amber and Hall, Aron J. and Wentworth, David E. and Silk, Benjamin J. and Thornburg, Natalie J. and Paden, Clinton R.},
  year = 2024,
