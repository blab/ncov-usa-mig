@article{leeOrderingQuantumDots2002,
  title = {Ordering of Quantum Dots Using Genetically Engineered Viruses.},
  author = {Lee, Seung-Wuk and Mao, Chuanbin and Flynn, Christine E and Belcher, Angela M},
  year = 2002,
  month = may,
  journal = {Science (New York, N.Y.)},
