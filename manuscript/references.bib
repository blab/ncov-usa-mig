@article{reederSpatialConcentrationPeacekeeping2018,
  title = {The {{Spatial Concentration}} of {{Peacekeeping Personnel}} and {{Public Health During Intrastate Conflicts}}},
  author = {Reeder, Bryce W.},
  year = 2018,
  month = may,
  journal = {International Peacekeeping},
  volume = {25},
