@article{beyerPerceivedNeighborhoodQuality2016,
  title = {Perceived {{Neighborhood Quality}} and {{Cancer Screening Behavior}}: {{Evidence}} from the {{Survey}} of the {{Health}} of {{Wisconsin}}},
