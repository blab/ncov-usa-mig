@article{fuSpatialAccessibilityProviders2009,
  title = {Spatial {{Accessibility}} to {{Providers}} and {{Vaccination Compliance Among Children With Medicaid}}},
  author = {Fu, Linda Y. and Cowan, Nuala and McLaren, Rosie and Engstrom, Ryan and Teach, Stephen J.},
  year = 2009,
