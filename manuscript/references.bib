@article{singhEnhancementVEGFmediatedAngiogenesis2011,
  title = {The Enhancement of {{VEGF-mediated}} Angiogenesis by Polycaprolactone Scaffolds with Surface Cross-Linked Heparin.},
  author = {Singh, Shivani and Wu, Benjamin M and Dunn, James C Y},
  year = 2011,
  month = mar,
  journal = {Biomaterials},
  volume = {32},
  number = {8},
