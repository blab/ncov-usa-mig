@article{kalyanaramanmarcelloDisaggregatingAsianRace2022,
  title = {Disaggregating {{Asian Race Reveals COVID-19 Disparities Among Asian American Patients}} at {{New York City}}'s {{Public Hospital System}}},
  author = {Kalyanaraman Marcello, Roopa and Dolle, Johanna and Tariq, Areeba and Kaur, Sharanjit and Wong, Linda and Curcio, Joan and Thachil, Rosy and Yi, Stella S. and Islam, Nadia},
  year = 2022,
  month = mar,
  journal = {Public Health Reports},
  volume = {137},
