@article{corneliusImpactFoodHousing2017,
  title = {Impact of Food, Housing, and Transportation Insecurity on {{ART}} Adherence: A Hierarchical Resources Approach},
  shorttitle = {Impact of Food, Housing, and Transportation Insecurity on {{ART}} Adherence},
  author = {Cornelius, Talea and Jones, Maranda and Merly, Cynthia and Welles, Brandi and Kalichman, Moira and Kalichman, Seth},
  year = 2017,
  journal = {AIDS Care},
  volume = {29},
