@article{bussolinoHepatocyteGrowthFactor1992,
  title = {Hepatocyte Growth Factor Is a Potent Angiogenic Factor Which Stimulates Endothelial Cell Motility and Growth.},
  author = {Bussolino, F and Di Renzo, M F and Ziche, M and Bocchietto, E and Olivero, M and Naldini, L and Gaudino, G and Tamagnone, L and Coffer, a and Comoglio, P M},
  year = 1992,
