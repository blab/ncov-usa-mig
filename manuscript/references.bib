@article{mcquellonQualityLifeMeasurement1997,
  title = {Quality of Life Measurement in Bone Marrow Transplantation: {{Development}} of the {{Functional Assessment}} of {{Cancer Therapy-Bone Marrow Transplant}} ({{FACT-BMT}}) Scale},
  author = {McQuellon, R P and Russell, G B and Cella, D F and Craven, B L and Brady, M and Bonomi, A and Hurd, D D},
  year = 1997,
  journal = {Bone Marrow Transplant},
  volume = {19},
