@article{espositoDogCatBiteassociated2013,
  title = {Dog and Cat Bite-Associated Infections in Children},
  author = {Esposito, S. and Picciolli, I. and Semino, M. and Principi, N.},
  year = 2013,
  journal = {European Journal of Clinical Microbiology and Infectious Diseases},
  volume = {32},
  number = {8},
  pages = {971--976},
  issn = {1009601318},
  doi = {10.1007/s10096-013-1840-x},
  abstract = {Millions of people throughout the world are bitten by animals each year. About 90 \% of the bites are caused by dogs and cats, and infections are the most common complications. As children are the most frequently bitten subjects, pediatricians should provide parents with everything they need to know in order to confront the problem. However, this does not seem to be case and, as the treatment of bite wounds is frequently inappropriate and delayed, the risk of acute infection and sequelae is increased. The main aim of this review is to discuss the epidemiology, microbiology, and clinical characteristics of infections due to dog and cat bites in children, and suggest the best approach to their management. Analysis of the published literature shows that prompt treatment is necessary in order to reduce the risk of infection. The therapeutic measures include wound washing, specific prophylaxis (i.e., tetanus and/or rabies), and antibiotics in the case of immunocompromised patients or deep wounds (particularly if there is evidence of edema or crushing), facial bites, or any wound over a tendon or bone. \copyright{} 2013 Springer-Verlag Berlin Heidelberg.}
}

