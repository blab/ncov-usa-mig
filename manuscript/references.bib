@article{meijerKineticAnalysisHexose1996,
  title = {Kinetic Analysis of Hexose Uptake in {{Saccharomyces}} Cerevisiae Cultivated in Continuous Culture},
  author = {Meijer, M. M. and Boonstra, J. and Verkleij, A. J. and Verrips, C. T.},
  year = 1996,
