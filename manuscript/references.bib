@article{ditahChangingEpidemiologyHepatitis2014,
  title = {The Changing Epidemiology of Hepatitis {{C}} Virus Infection in the {{United States}}: {{National}} Health and Nutrition Examination Survey 2001 through 2010},
  author = {Ditah, Ivo and Ditah, Fausta and Devaki, Pardha and Ewelukwa, Oforbuike and Ditah, Chobufo and Njei, Basile and Luma, Henry N. and Charlton, Michael},
  year = 2014,
  journal = {Journal of Hepatology},
  volume = {60},
