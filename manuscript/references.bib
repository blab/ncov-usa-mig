@article{leeControlledDualRelease2008,
  title = {Controlled Dual Release of Basic Fibroblast Growth Factor and Indomethacin from Heparin-Conjugated Polymeric Micelle.},
  author = {Lee, Jung Seok and Bae, Jin Woo and Joung, Yoon Ki and Lee, Seung Jin and Han, Dong Keun and Park, Ki Dong},
