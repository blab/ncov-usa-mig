@article{jarlenskiTrendsDisparitiesLowIncome2016,
  title = {Trends in {{Disparities}} in {{Low-Income Children}}'s {{Health Insurance Coverage}} and {{Access}} to {{Care}} by {{Family Immigration Status}}},
  author = {Jarlenski, Marian and Baller, Julia and Borrero, Sonya and Bennett, Wendy L.},
