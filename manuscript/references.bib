  shorttitle = {Continuous vs {{Intermittent}} {$\beta$}-{{Lactam Antibiotic Infusions}} in {{Critically Ill Patients With Sepsis}}},
  author = {Dulhunty, Joel M. and Brett, Stephen J. and De Waele, Jan J. and Rajbhandari, Dorrilyn and Billot, Laurent and Cotta, Menino O. and Davis, Joshua S. and Finfer, Simon and Hammond, Naomi E. and Knowles, Serena and Liu, Xiaoqiu and McGuinness, Shay and Mysore, Jayanthi and Paterson, David L. and Peake, Sandra and Rhodes, Andrew and Roberts, Jason A. and Roger, Claire and Shirwadkar, Charudatt and Starr, Therese and Taylor, Colman and Myburgh, John A. and Lipman, Jeffrey and {BLING III Study Investigators}},
  year = 2024,
