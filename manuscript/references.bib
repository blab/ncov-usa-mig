  author = {Iversen, Kasper and Ihlemann, Nikolaj and Gill, Sabine U. and Madsen, Trine and Elming, Hanne and Jensen, Kaare T. and Bruun, Niels E. and H{\o}fsten, Dan E. and Fursted, Kurt and Christensen, Jens J. and Schultz, Martin and Klein, Christine F. and Fosb{\o}ll, Emil L. and Rosenvinge, Flemming and Sch{\o}nheyder, Henrik C. and K{\o}ber, Lars and {Torp-Pedersen}, Christian and {Helweg-Larsen}, Jannik and T{\o}nder, Niels and Moser, Claus and Bundgaard, Henning},
