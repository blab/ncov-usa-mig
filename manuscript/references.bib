@article{nimgaonkarOutcomesHematopoieticCell2024,
  title = {Outcomes in {{Hematopoietic Cell Transplant}} and {{Chimeric Antigen Receptor T-Cell Therapy Recipients With Pre}}--{{Cellular Therapy SARS-CoV-2 Infection}}},
  author = {Nimgaonkar, Ila and Yoke, Leah H and Roychoudhury, Pavitra and Flaherty, Patrick W and Oshima, Masumi Ueda and Weixler, Amelia and Gauthier, Jordan and Greninger, Alexander L and Mielcarek, Marco and Boeckh, Michael and Liu, Catherine and Hill, Joshua A},
