@article{suAssociationRaceEthnicity2017,
  title = {The Association between Race/Ethnicity and the Effectiveness of Direct Antiviral Agents for Hepatitis {{C}} Virus Infection},
  author = {Su, Feng and Green, Pamela K. and Berry, Kristin and Ioannou, George N.},
  year = 2017,
  journal = {Hepatology},
  volume = {65},
