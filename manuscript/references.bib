@article{beyerCharacteristicsResidentialNeighborhood2013,
  title = {Characteristics of the Residential Neighborhood Environment Differentiate Intimate Partner Femicide in Urban versus Rural Settings},
  author = {Beyer, Kirsten M. M. and Layde, Peter M. and Hamberger, L. Kevin and Laud, Purushottam W.},
