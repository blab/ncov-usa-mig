@article{pappCDCLaboratoryRecommendations2024,
  title = {{{CDC Laboratory Recommendations}} for {{Syphilis Testing}}, {{United States}}, 2024},
  author = {Papp, John R. and Park, Ina U. and Fakile, Yetunde and Pereira, Lara and Pillay, Allan and Bolan, Gail A.},
  year = 2024,
