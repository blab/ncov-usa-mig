@article{chanHighperformanceLithiumBattery2008,
  title = {High-Performance Lithium Battery Anodes Using Silicon Nanowires.},
  author = {Chan, Candace K and Peng, Hailin and Liu, Gao and McIlwrath, Kevin and Zhang, Xiao Feng and a Huggins, Robert and Cui, Yi},
  year = 2008,
  month = jan,
  journal = {Nature nanotechnology},
