@article{wilkinsonRecurrentSARSCoV2Mutations2022,
  title = {Recurrent {{SARS-CoV-2}} Mutations in Immunodeficient Patients},
  author = {Wilkinson, S A J and Richter, Alex and Casey, Anna and Osman, Husam and Mirza, Jeremy D and Stockton, Joanne and Quick, Josh and Ratcliffe, Liz and Sparks, Natalie and Cumley, Nicola and Poplawski, Radoslaw and Nicholls, Samuel N and Kele, Beatrix and Harris, Kathryn and Peacock, Thomas P and Loman, Nicholas J and {The COVID-19 Genomics UK (COG-UK) consortium}},
  year = 2022,
