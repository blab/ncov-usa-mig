  title = {Smooth Muscle Cell Growth in Photopolymerized Hydrogels with Cell Adhesive and Proteolytically Degradable Domains: Synthetic {{ECM}} Analogs for Tissue Engineering.},
  author = {Mann, B K and Gobin, a S and Tsai, a T and Schmedlen, R H and West, J L},
