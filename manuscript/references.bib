@article{artinyanRaceEthnicitySocioeconomic2010,
  title = {Race, Ethnicity, and Socioeconomic Status Influence the Survival of Patients with Hepatocellular Carcinoma in the {{United States}}},
  author = {Artinyan, Avo and Mailey, Brian and {Sanchez-Luege}, Nicelio and Khalili, Joshua and Sun, Can Lan and Bhatia, Smita and Wagman, Lawrence D. and Nissen, Nicholas and Colquhoun, Steven D. and Kim, Joseph},
