  keywords = {Care Coordination,Health,Interviews,Transportation Disadvantage},
  file = {/Users/abemania/Zotero/storage/MJCXM5SR/Oluyede et al. - 2022 - Unpacking transportation barriers and facilitators.pdf;/Users/abemania/Zotero/storage/7N36N4YJ/S2590198222000288.html}
