@article{hamreCommuterModeChoice2014,
  title = {Commuter {{Mode Choice}} and {{Free Car Parking}}, {{Public Transportation Benefits}}, {{Showers}}/{{Lockers}}, and {{Bike Parking}} at {{Work}}: {{Evidence}} from the {{Washington}}, {{DC Region}}},
