  author = {Yang, Baiyu and Liu, Jessica B. and So, Samuel K. and Han, Summer S. and Wang, Sophia S. and Hertz, Andrew and {Shariff-Marco}, Salma and Lin Gomez, Scarlett and Rosenberg, Philip S. and Nguyen, Mindie H. and Hsing, Ann W.},
  year = 2018,
  journal = {Cancer},
