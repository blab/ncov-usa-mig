  author = {Charu, Vivek and Zeger, Scott and Gog, Julia and Bj{\o}rnstad, Ottar N. and Kissler, Stephen and Simonsen, Lone and Grenfell, Bryan T. and Viboud, C{\'e}cile},
  year = 2017,
