@article{salomeCongenitalCytomegalovirusInfection2023,
  title = {Congenital Cytomegalovirus Infection: The State of the Art and Future Perspectives},
  shorttitle = {Congenital Cytomegalovirus Infection},
  author = {Salom{\`e}, S. and Corrado, F. R. and Mazzarelli, L. L. and Maruotti, G. M. and Capasso, L. and {Blazquez-Gamero}, D. and Raimondi, F.},
  year = 2023,
