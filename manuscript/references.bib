  keywords = {administration & dosage,Aged,Alanine Transaminase,analogs & derivatives,Apoptosis,Aspartate Aminotransferases,blood,Caffeine,Chronic,Coffee,Collagen,Cross-Over Studies,Deoxyguanosine,drug effects,enzymology,Female,Hepacivirus,Hepatitis C,Humans,Male,Middle Aged,Oxidative DNA damage,prevention & control,Prospective Studies,RNA,Telomere,ultrastructure,Viral,virology},
  file = {/Users/abemania/Zotero/storage/PSSD3A7A/Cardin et al. - 2013 - Effects of coffee consumption in chronic hepatitis C A randomized controlled trial.pdf}
}

@article{carrieriAssociationElevatedCoffee2014,
  title = {Association between Elevated Coffee Consumption and Daily Chocolate Intake with Normal Liver Enzymes in {{HIV-HCV}} Infected Individuals: {{Results}} from the {{ANRS CO13 HEPAVIH}} Cohort Study},
  author = {Carrieri, M. Patrizia and Lions, Caroline and Sogni, Philippe and Winnock, Maria and Roux, Perrine and Mora, Marion and Bonnard, Philippe and Salmon, Dominique and Dabis, Fran??ois and Spire, Bruno},
  year = 2014,
  journal = {Journal of Hepatology},
  volume = {60},
