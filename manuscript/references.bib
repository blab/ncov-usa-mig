  author = {Kresnowati, M T a P and a {van Winden}, W and Almering, M J H and {ten Pierick}, a and Ras, C and a Knijnenburg, T and {Daran-Lapujade}, P and Pronk, J T and Heijnen, J J and Daran, J M},
  year = 2006,
