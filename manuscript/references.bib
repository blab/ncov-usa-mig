@article{bartenReducedEmergencyDepartment2022,
  title = {Reduced {{Emergency Department Utilization During}} the {{Early Phase}} of the {{COVID-19 Pandemic}}: {{Viral Fear}} or {{Lockdown Effect}}?},
  shorttitle = {Reduced {{Emergency Department Utilization During}} the {{Early Phase}} of the {{COVID-19 Pandemic}}},
  author = {Barten, Dennis G. and Latten, Gideon H.P. and {van Osch}, Frits H.M.},
