@article{tongCAMERA2CombinationAntibiotic2016,
  title = {{{CAMERA2}} -- Combination Antibiotic Therapy for Methicillin-Resistant {{Staphylococcus}} Aureus Infection: Study Protocol for a Randomised Controlled Trial},
  shorttitle = {{{CAMERA2}} -- Combination Antibiotic Therapy for Methicillin-Resistant {{Staphylococcus}} Aureus Infection},
  author = {Tong, Steven Y. C. and Nelson, Jane and Paterson, David L. and Fowler, Vance G. and Howden, Benjamin P. and Cheng, Allen C. and Chatfield, Mark and Lipman, Jeffrey and Van Hal, Sebastian and O'Sullivan, Matthew and Robinson, James O. and Yahav, Dafna and Lye, David and Davis, Joshua S. and {for the CAMERA2 study group and the Australasian Society for Infectious Diseases Clinical Research Network}},
