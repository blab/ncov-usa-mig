  author = {Mauad, T H and {van Nieuwkerk}, C M and Dingemans, K P and Smit, J J and Schinkel, A H and Notenboom, R G and {van den Bergh Weerman}, M A and Verkruisen, R P and Groen, A K and Oude Elferink, R P},
