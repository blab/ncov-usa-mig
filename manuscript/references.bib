@article{braviCoffeeReducesRisk2013,
  title = {Coffee Reduces Risk for Hepatocellular Carcinoma: An Updated Meta-Analysis.},
  author = {Bravi, Francesca and Bosetti, Cristina and Tavani, Alessandra and Gallus, Silvano and La Vecchia, Carlo},
