@article{poloSpatialAccessibilityVaccination2013,
  title = {Spatial Accessibility to Vaccination Sites in a Campaign against Rabies in {{S\~ao Paulo}} City, {{Brazil}}},
  author = {Polo, Gina and Acosta, Carlos Mera and Dias, Ricardo Augusto},
  year = 2013,
  month = aug,
  journal = {Preventive Veterinary Medicine},
  volume = {111},
  number = {1-2},
