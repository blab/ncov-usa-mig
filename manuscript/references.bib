@article{yokoyamaAlcoholrelatedCancersAldehyde1998,
  title = {Alcohol-Related Cancers and Aldehyde Dehydrogenase-2 in {{Japanese}} Alcoholics},
  author = {Yokoyama, Akira and Muramatsu, Taro and Ohmori, Tai and Yokoyama, Tetsuji and Okuyama, Keiji and Takahashi, Hisao and Hasegawa, Yoshio and Higuchi, Susumu and Maruyama, Katsuya and Shirakura, Katsuyuki and Ishii, Hiromasa},
  year = 1998,
