@article{michelottiNAFLDNASHLiver2013,
  title = {{{NAFLD}}, {{NASH}} and Liver Cancer},
  author = {Michelotti, Gregory A. and Machado, Mariana V. and Diehl, Anna Mae},
  year = 2013,
  journal = {Nature Reviews Gastroenterology and Hepatology},
  volume = {10},
  number = {11},
  pages = {656--665},
  publisher = {Nature Publishing Group},
  issn = {1759-5045},
  doi = {10.1038/nrgastro.2013.183},
  abstract = {NAFLD affects a large proportion of the US population and its incidence and prevalence are increasing to epidemic proportions around the world. As with other liver diseases that cause cirrhosis, NAFLD increases the risk of liver cancer, a disease with poor outcomes and limited therapeutic options. The incidences of hepatocellular carcinoma (HCC) and intrahepatic cholangiocarcinoma are also rising, and HCC is now the leading cause of obesity-related cancer deaths in middle-aged men in the USA. In this Review, we summarize the correlations between liver cancer and NAFLD-related cirrhosis, and the role of the metabolic syndrome in the development of liver cancer from diverse aetiologies, including HCV-mediated cirrhosis. Recent advances in understanding the progression of NAFLD to HCC from preclinical models will also be discussed. Targeted genetic manipulation of certain metabolic or stress-response pathways, including one-carbon metabolism, NF-{$\kappa$}B, PTEN and microRNAs, has been valuable in elucidating the pathways that regulate carcinogenesis in NAFLD. Although tremendous advances have occurred in the identification of diagnostic and therapeutic opportunities to reduce the progression of NAFLD, considerable gaps in our knowledge remain with regard to the mechanisms by which NAFLD and its risk factors promote liver cancer.}
}

@article{michikawaDevelopmentPredictionModel2012,
  title = {Development of a Prediction Model for 10-Year Risk of Hepatocellular Carcinoma in Middle-Aged {{Japanese}}: The {{Japan Public Health Center-based Prospective Study Cohort II}}.},
  author = {Michikawa, Takehiro and Inoue, Manami and Sawada, Norie and Iwasaki, Motoki and Tanaka, Yasuhito and Shimazu, Taichi and Sasazuki, Shizuka and Yamaji, Taiki and Mizokami, Masashi and Tsugane, Shoichiro},
  year = 2012,
  month = aug,
  journal = {Preventive medicine},
  volume = {55},
  number = {2},
  pages = {137--143},
  doi = {10.1016/j.ypmed.2012.05.017},
  abstract = {OBJECTIVE: The purpose of the present study was to develop a risk estimation model for the 10-year risk of hepatocellular carcinoma (HCC) that could be easily used in a general population to aid in the prevention of HCC. METHODS: Our prediction model was derived from data obtained on 17,654 Japanese aged 40 to 69 years who participated in health checkups (follow-up: 1993-2006). Cox proportional hazards regression was applied to obtain coefficients for each predictor. RESULTS: During follow-up, a total of 104 cases of HCC were newly diagnosed. After checking the model fit, we incorporated age, sex, alcohol consumption, body mass index, diabetes, coffee consumption, and hepatitis B and C virus infection into the prediction model. The model showed satisfactory discrimination (Harrell's c-index=0.94) and was well calibrated (the overall observed/expected ratio=1.03, 95\% confidence interval=0.83-1.29). We also developed a simple risk scoring system. Those subjects with total scores of 17 or more under this system (score range: -1 to 19) had an estimated 10-year HCC risk of over 90\%; those with 4 points or less had an estimated risk of less than 0.1\%. CONCLUSION: We developed a simple 10-year risk prediction model for HCC in the Japanese general population as a public education tool.},
  langid = {english},
  keywords = {Adult,Age Distribution,Age Factors,Aged,Alcohol Drinking,Body Mass Index,Carcinoma Hepatocellular,Coffee,Cohort Studies,Drinking Behavior,epidemiology,etiology,Female,Follow-Up Studies,Health Knowledge Attitudes Practice,Hepatitis B,Hepatitis C,Humans,Japan,Liver Neoplasms,Male,methods,Middle Aged,Predictive Value of Tests,Proportional Hazards Models,Prospective Studies,Risk Assessment,Sex Distribution,Surveys and Questionnaires}
